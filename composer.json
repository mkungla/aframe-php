{
	"name" : "mkungla/aframe-php",
	"description" : "Work in progress...",
	"type" : "composer-plugin",
	"license" : "MIT",
    "require": {
        	"php": ">= 7",
        	"composer-plugin-api": "^1.1",
        	"mkungla/aframe":"*"
    },
    "minimum-stability" : "stable",
	"autoload" : {
		"psr-4" : {
			"AframeVR\\" : "src/"
		}
	},
	"autoload-dev": {
	    "psr-4": {
	        "AframeVR\\Tests\\": "tests/helpers"
	    }
	},
	"config" : {
		"optimize-autoloader" : true,
		"aframe-dir": "public/aframe",
		"aframe-url": "/aframe"
	},
	"homepage" : "https://github.com/mkungla/aframe-php",
	"authors" : [{
			"name" : "Marko Kungla",
			"email" : "marko@okramlabs.com",
			"homepage" : "https://github.com/mkungla",
			"role" : "Developer"
		}
	],
	"support" : {
		"issues" : "https://github.com/mkungla/aframe-php/issues",
		"source" : "https://github.com/mkungla/aframe-php"
	},
	"keywords" : [
		"php",
		"aframe"
	],
	"extra": {
        "class": "AframeVR\\Composer\\Installer\\AframeInstallerPlugin"
    }
}
